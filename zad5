def sprawdz_wartosc_na_liscie(lista, wartosc):
    return wartosc in lista

moja_lista = [1, 2, 3, 4, 5]
wartosc_do_sprawdzenia = 3

wynik = sprawdz_wartosc_na_liscie(moja_lista, wartosc_do_sprawdzenia)

if wynik:
    print(f'Wartość {wartosc_do_sprawdzenia} znajduje się na liście.')
else:
    print(f'Wartość {wartosc_do_sprawdzenia} nie znajduje się na liście.')
